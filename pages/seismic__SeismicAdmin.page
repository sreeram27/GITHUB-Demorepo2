                            fieldsToCheck[buttonNameId] = '{!$Label.seismic__commonbuttonname}';
                            if({!ISNULL(newObjectTypeMapping.Id)}) {
                                fieldsToCheck[objectName] = '{!$Label.seismic__commonrelated}';
