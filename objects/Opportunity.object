    <validationRules>
        <fullName>Close_Date_Updation</fullName>
        <active>true</active>
        <description>Close Date is mandate since the opportunity is in &apos;Procurement&apos;, &apos;Closed Won&apos;, &apos;Closed Won Implementation&apos;, &apos;Closed Lost&apos;</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(StageName ,&apos;Procurement&apos;),
ISPICKVAL(StageName ,&apos;Closed Won&apos;),
ISPICKVAL(StageName ,&apos;Closed Won Implementation&apos;),
ISPICKVAL(StageName,&apos;Closed Lost&apos;), 
ISBLANK(&apos;CloseDate&apos;)
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date is mandate during this sales stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Reason_should_be_empty</fullName>
        <active>true</active>
        <description>Closed Lost Reason should be empty  for all stages except for Closed Lost</description>
        <errorConditionFormula>AND( NOT(ISBLANK( Closed_Lost_Reason__c )),NOT(ISPICKVAL( StageName ,&apos;Closed Lost&apos;) ) )</errorConditionFormula>
        <errorMessage>Closed Lost Reason should be empty for all stages except for Closed Lost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_should_have_a_reason</fullName>
        <active>true</active>
        <description>Whenever Opp is marked is Closed Lost then Closed Lost Reason should be populated</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName ,&apos;Closed Lost&apos;) , ISBLANK( Closed_Lost_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>Closed_Lost_Reason__c</errorDisplayField>
        <errorMessage>Opportunity cannot be Closed Lost without a Closed Lost Reason.Please make sure Closed Lost Reason is  not empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Validataion_Rule</fullName>
        <active>false</active>
        <description>Applies only on Closed Won</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Closed Won&apos;),
OR(
ISBLANK(TEXT( No_of_Subscriptions__c )),
ISBLANK(TEXT( Professional_Services_hours__c )),
ISBLANK(Any_commitments_from_AR__c),
ISBLANK(TEXT(Premium_Support__c)),
ISBLANK(TEXT(Plan_Details__c)),
ISBLANK(TEXT(Hosting_Type__c))))</errorConditionFormula>
        <errorMessage>Please fill in Subs Activation Details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Competitior_Info_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>ISBlank(Competitors__c)</errorConditionFormula>
        <errorMessage>Please insert the name of the competitors. If there are no competitors, please choose None as an option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Subs_cannot_be_zero</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
ISPICKVAL(StageName, &quot;Closed Won&quot;),
ISPICKVAL(StageName, &quot;Closed Won-Implementation Incomplete&quot;))
,
OR(
ISBLANK( Current_Subs__c ),
Current_Subs__c =0
)
)</errorConditionFormula>
        <errorDisplayField>Current_Subs__c</errorDisplayField>
        <errorMessage>Current Subs cannot be zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dates_Validation_for_New_logos</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 New__c  = TRUE &amp;&amp;  isFromOpportunity__c = False  ,
OR(

 ISBLANK( Demo_Performed_Date__c),
 ISBLANK( Demo_Scheduled_Date__c)
)
)</errorConditionFormula>
        <errorDisplayField>of_days_in_Negotiation__c</errorDisplayField>
        <errorMessage>DC and Demo dates are mandatory for &quot;New Logos&quot;. Please update to continue saving !!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Partner_Involved_No</fullName>
        <active>true</active>
        <description>Rule to prevent users entering Primary Partner Section if Partner Involved is chosen as &apos;No&apos;.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Partner__c, &apos;No&apos;) ,
AND(
NOT( ISBLANK( Partner_Name_New__c )),
NOT( ISBLANK( Partner_Contact__c )), NOT(ISBLANK(TEXT(Partner_Influence__c))),
NOT( ISBLANK(TEXT( Partner_Sourced__c))) 
)
)</errorConditionFormula>
        <errorMessage>Please select &apos;Partner Involved?&apos; as &apos;Yes&apos; to enter data in &apos;Partner Name&apos;, &apos;Partner Contact&apos;, &apos;Partner Influence&apos; and &apos;Partner Sourced&apos; fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Partner_Involved_Yes</fullName>
        <active>true</active>
        <description>Rule to mandate &apos;Partner Name&apos;, Partner Contact, Partner Sourced&apos;, and &apos;Partner Influence&apos; fields if &apos;Partner Involved&apos; is selected as &apos;Yes&apos;.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Partner__c , &apos;Yes&apos;), 
OR( 
ISBLANK(Partner_Name_New__c),
ISBLANK( Partner_Contact__c ),
ISBLANK( TEXT(Partner_Sourced__c )),
ISBLANK( TEXT(Partner_Influence__c)) 
)
)</errorConditionFormula>
        <errorMessage>&apos;Partner Name&apos; , &apos;Partner Contact&apos;, &apos;Partner Sourced&apos;, and &apos;Partner Influence&apos; are required when &apos;Partner Involved?&apos; is selected as &apos;Yes&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Partner_Sourced_is_Yes</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Partner_Sourced__c , &apos;Yes&apos;), 
ISBLANK( TEXT(Partner_Namee__c) ), 
ISBLANK( TEXT(Partner_Type__c) ), 
ISBLANK( Partner_Contact_Details__c), 
ISBLANK( TEXT(Partner_Influence__c) ), 
ISBLANK( TEXT(Partner_Role__c) ) 
)</errorConditionFormula>
        <errorMessage>Please fill all the partner related details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Implementation_Start_Date_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT (ISBLANK(Implementation_Closed_Date__c) ),
			
  		 ISCHANGED(Implementation_Start_Date__c) 	)</errorConditionFormula>
        <errorDisplayField>Implementation_Start_Date__c</errorDisplayField>
        <errorMessage>Implementation start date can&apos;t be modified after Implementation close date entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Records_to_Prevent_Other_User_Edits</fullName>
        <active>false</active>
        <errorConditionFormula>AND($User.ProfileId = Owner.Profile.Id,  $Profile.Name ==&apos;System Adminstrator&apos;)</errorConditionFormula>
        <errorMessage>Only the Owner of the Opp can make changes to the record. Please contact the Owner of the record for further action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Partner_ContactDetails_Influence</fullName>
        <active>false</active>
        <description>Rule to mandate &apos;Partner Contact Details&apos; and &apos;Partner Influence&apos; only when Partner Sourced is selected as &apos;Yes&apos;.</description>
        <errorConditionFormula>AND(
 TEXT(Partner_Sourced__c) = &quot;Yes&quot;,

OR( ISBLANK( Partner_Contact_Details__c ),
  TEXT(Partner_Influence__c ) = &quot; &quot;
) 
)</errorConditionFormula>
        <errorMessage>Please enter Partner Contact Details and Partner Influence because you have chosen Partner Sourced as &apos;Yes&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_or_Upsell_or_Renewal</fullName>
        <active>false</active>
        <errorConditionFormula>AND(New__c==false,  Renewal__c ==false ,Upsell__c==false )</errorConditionFormula>
        <errorMessage>Its mandatory to select any one among New, Up-sell or Renewal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Line_Items_not_Equal_to_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(ISPICKVAL(StageName, &quot;Closed Won&quot;),
ISPICKVAL(StageName, &quot;Closed Won-Implementation Incomplete&quot;)),
HasOpportunityLineItem = FALSE
)</errorConditionFormula>
        <errorMessage>Opportunity cannot be closed without Product line items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PG_set_madatory_on_PG_stage</fullName>
        <active>false</active>
        <description>The &quot;Playground Set&quot; field is mandatory when stage is Playground</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Playground&apos;),ISBLANK(TEXT( Playground_Set__c)))</errorConditionFormula>
        <errorDisplayField>Playground_Set__c</errorDisplayField>
        <errorMessage>Playground set is a mandatory field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PartnerNameNotOther_OtherPartnerBlank</fullName>
        <active>false</active>
        <description>Validation rule to ensure data for &apos;Other Partner?&apos; field only when the value for the &apos;Partner Name&apos; selected as &apos;Other&apos;.</description>
        <errorConditionFormula>AND (
  Validation_Rule_Bypass_c__c  = FALSE,
TEXT( Partner_Namee__c )  &lt;&gt;  &quot;Other&quot;,
 NOT( ISBLANK( Other_Partner__c ) )
)</errorConditionFormula>
        <errorDisplayField>Partner_Namee__c</errorDisplayField>
        <errorMessage>Select Partner Name as &apos;Other&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Plan_Details_Validation_Rule</fullName>
        <active>true</active>
        <description>Mandate on stage &apos;Closed Won&apos; or &apos;Closed Won Implementation&apos;</description>
        <errorConditionFormula>AND ( 
ISPICKVAL( StageName , &apos;Closed Won&apos;),
ISPICKVAL( StageName , &apos;Closed Won-Implementation Incomplete&apos;),
ISPICKVAL( Plan_Details__c,&apos;&apos; ) 
)</errorConditionFormula>
        <errorMessage>Please update the plan details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_Change_the_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCLONE(), ISPICKVAL( StageName , &apos;Closed Won&apos;) )</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Please change the Stage before adding Products.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premium_support_field_mandatory</fullName>
        <active>false</active>
        <description>This rule will make the premium support field mandatory if the stage is set to Closed Won</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Closed Won&apos;),ISBLANK(TEXT( Is_eligibile_for_Premier_support__c)))</errorConditionFormula>
        <errorDisplayField>Is_eligibile_for_Premier_support__c</errorDisplayField>
        <errorMessage>This field is required for Closed Won Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Current_Subs_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(INCLUDES(Opportunity_Type_New_Renewal_Upsell__c , &quot;Renewal&quot;),INCLUDES(Opportunity_Type_New_Renewal_Upsell__c , &quot;Upsell&quot;)), OR(ISPICKVAL(StageName , &quot;Closed Won&quot;) ,ISPICKVAL(StageName , &quot;Closed Won-Implementation Incomplete&quot;)) ,
ISBLANK( Current_Subs__c )
)</errorConditionFormula>
        <errorDisplayField>Current_Subs__c</errorDisplayField>
        <errorMessage>Please enter current subs of previous oppty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_to_edit_Oppty_Contract_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>OR ( 
(
ISNEW() &amp;&amp; NOT(ISCLONE()) &amp;&amp; NOT(ISBLANK(Contract_Start_Date__c)) &amp;&amp; 
NOT(ISBLANK(Contract_End_Date__c)) &amp;&amp; NOT( $Permission.To_Edit_Opportunity_Contract_Start_and_End_Date )   
),
(
(ISCHANGED( Contract_Start_Date__c ) || ISCHANGED( Contract_End_Date__c )) &amp;&amp; (NOT( $Permission.To_Edit_Opportunity_Contract_Start_and_End_Date ))
)
)</errorConditionFormula>
        <errorMessage>Not authorized to edit Contract Start Date and/or Contract End Date. Please contact Customer Success team for help.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subs_Activation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
ISPICKVAL(StageName, &quot;Closed Won&quot;),
ISPICKVAL(StageName, &quot;Closed Won-Implementation Incomplete&quot;))
,
OR(
ISBLANK( Current_Subs__c),
ISBLANK (Professional_Services_hours__c),
ISBLANK(TEXT(Premium_Support__c)),
ISBLANK(Tentative_Activation_Date__c),
ISBLANK(Any_commitments_from_AR__c)

)
)</errorConditionFormula>
        <errorMessage>Please fill in the details on &quot;Subs Activation Section&quot; to continue saving !!

This would enable an auto email to Subscriptions@autorabit.com instead of manually sending.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subs_Activation_Rule_Current_Subs</fullName>
        <active>true</active>
        <description>Validation Rule for Current Subs</description>
        <errorConditionFormula>AND(OR(
ISPICKVAL(StageName, &quot;Closed Won&quot;),
ISPICKVAL(StageName, &quot;Closed Won-Implementation Incomplete&quot;))
,
ISBLANK( Current_Subs__c)
)</errorConditionFormula>
        <errorMessage>Please fill in the &quot;Current Subs&quot; field to continue saving !!

Current Subs = Count of Previous licenses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>To_Mandate_Other_Partner</fullName>
        <active>true</active>
        <description>Validation rule to mandate &apos;Other Partner&apos; field if the value for &apos;Partner Name&apos; is selected as &apos;Other&apos;</description>
        <errorConditionFormula>AND(
ISPICKVAL( Partner_Namee__c , &quot;Other&quot;),

ISBLANK(Other_Partner__c)

)</errorConditionFormula>
        <errorDisplayField>Other_Partner__c</errorDisplayField>
        <errorMessage>Enter Other Partner name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VC_Type_mandatory_on_PG_stage</fullName>
        <active>false</active>
        <description>The &quot;Version Control Type&quot; field is mandatory on Playground stage.</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName,&apos;Playground&apos;),ISBLANK(TEXT( Version_Control_Type__c )))</errorConditionFormula>
        <errorDisplayField>Version_Control_Type__c</errorDisplayField>
        <errorMessage>Version Control Type is a mandatory field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(IT_Implementation_Stage__c,&apos;Training&apos;),ISBLANK(TEXT(  Implementation_Start_Date__c )))</errorConditionFormula>
        <errorDisplayField>Implementation_Start_Date__c</errorDisplayField>
        <errorMessage>Implementation Start Date is mandatory if it is in Training Stage</errorMessage>
    </validationRules>
