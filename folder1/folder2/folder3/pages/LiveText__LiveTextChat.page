    function redirectPrimaryTab(phoneNumber){
        sforce.console.getFocusedPrimaryTabId(function(result) {
            // W-5706341 - If result.success == false then I get the tabId from the document.cookie
            var tabId = result.sucess ? result.id : getCookie('tabidfirstoutboundmsg');
            if (!tabId || 0 === tabId.length) {
                console.log('redirectPrimaryTab - TabId could not be retrieved. Redirect to Primary Tab not possible');
            } else {
                sforce.console.getPageInfo(tabId, function(result){
                    var resultObj =  JSON.parse(result.pageInfo);
                    console.log('resultObj: ' + JSON.stringify(result.pageInfo));

                    var pageUrl = resultObj.url;
                    var firstPart = pageUrl.substring(0,pageUrl.indexOf('?')+1);
                    var paramNames = getUrlParameterNames(pageUrl);
                    var newUrl = firstPart;
                    for(var i=0;i<paramNames.length;i++){
                        var paramName = paramNames[i];
                        if(i > 0)
                            newUrl += '&';
                        newUrl += paramName;
                        newUrl += '=';
                        if(paramName === 'phone'){
                            newUrl += phoneNumber;
                        }else if(paramName === 'cvid'){
                            newUrl += currentConversationRecordId;
                        }else{
                            newUrl += getSafeParameterByName(paramName, pageUrl);
                        }
                    }
                    // W-5706341 - This check is to avoid error 'match' not found in null, 
                    // happens when newUrl is empty in openPrimaryTab event
                    if (!newUrl || 0 === newUrl.length) {
                        console.log('Newurl from primary tab is empty, cancel openPrimaryTab event');
                        return;
                    } else {
                        sforce.console.openPrimaryTab(tabId, newUrl, true);
                    }
                });
            }
        });
    }

